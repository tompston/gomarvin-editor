{
    "name": "gomarvin-editor",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "init-build": "vue-tsc --noEmit && vite build",
        "build": "vite build",
        "preview": "npm run build && serve dist -p 8282",
        "postbuild": "purgecss --css dist/assets/*.css --content dist/assets/*.js -o dist/assets/  --safelist html body"
    },
    "dependencies": {
        "vue": "^3.2.25",
        "vue-router": "^4.0.14"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^2.2.0",
        "purgecss": "^4.1.3",
        "typescript": "^4.5.4",
        "vite": "^2.8.0",
        "vue-tsc": "^0.29.8"
    }
}
